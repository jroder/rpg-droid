<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thing.rpg_droid.res" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.thing.rpg_droid.app.Activity_CharacterList"
            android:label="@string/title_activity_activity__character_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thing.rpg_droid.app.Activity_Charsheet"
            android:label="@string/app_name"
            android:parentActivityName="com.thing.rpg_droid.app.Activity_CharacterList" >
        </activity>
        <activity
            android:name="com.thing.rpg_droid.app.Activity_Settings"
            android:label="@string/title_activity_activity__settings"
            android:parentActivityName="com.thing.rpg_droid.app.Activity_CharacterList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="Activity_CharacterList" />
        </activity>
    </application>

</manifest>
